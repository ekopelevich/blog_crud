<h1>{{pageTitle}}</h1>

{{#each users}}
  <section class="user-container">
    <img class="user-image circle" src="{{this.image_url}}" alt="{{this.title}}" />
    <section class="user-info">
      <h3 class="user-name">{{this.first_name}} {{this.last_name}}</h3>
      <p>{{this.bio}}</p>
    </section>
  </section>
{{/each}}
